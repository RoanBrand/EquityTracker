    <script src="js/{{ .Code }}"></script>

    <style>
        .slick-header-column {
            min-height: 25px !important;
        }

        @media print {
            .print-hide {
                display: none;
            }
        }
    </style>
</head>
<body>
{{ template "layout" }}
</body>
</html>
{{ end }}
