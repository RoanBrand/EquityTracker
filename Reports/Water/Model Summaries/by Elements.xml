<?xml version="1.0" encoding="UTF-8"?>
<report>
    <name>water-modelsummaries-byelements</name>
    <module>Water</module>
    <category>Model Summaries</category>
    <title>by Elements</title>
    <parameters>
        <parameter>IMQS_scenario</parameter>
        <parameter>Element</parameter>
    </parameters>
    <datasource>
        <dbname>reports</dbname>
        <viewtable>
            <tablename>water_model_summaries_elements</tablename>
            <fields>
                <field>Capacity (kL,kL/d)</field>
                <field>Count</field>
                <field>Elements</field>
                <field>Length (m)</field>
                <field>OrderIndex</field>
                <field>Output (l/s)</field>
                <field>Replace Value (R)</field>
                <field>SecondOrder</field>
                <field>scenario</field>
            </fields>
            <filter>
                <condition>"scenario" = {IMQS_scenario}</condition>
            </filter>
        </viewtable>
    </datasource>
    <layout>
        <toolbar></toolbar>
        <export>PDF</export>
        <tab>
            <name>Dashboard</name>
            <template>content-dashboard</template>
        </tab>
        <tab>
            <name>Summary</name>
            <template>content-summary</template>
        </tab>
    </layout>
</report>
